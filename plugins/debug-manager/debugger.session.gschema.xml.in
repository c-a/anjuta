<schemalist>
  <schema id="debugger" path="/debugger/">
    <key type="as" name="source-directories">
      <default>[]</default>
      <summary>List of source directories</summary>
    </key>
    <key type="b" name="stop-at-beginning">
      <default>false</default>
      <summary>Whether the debugger should stop at the beginning of program execution</summary>
    </key>
    <key type="s" name="remote-debugger">
      <default>""</default>
      <summary>Remote debugger string</summary>
    </key>
    <key type="as" name="watches">
      <default>[]</default>
      <summary>List of watches</summary>
    </key>
    <key type="as" name="breakpoints">
      <default>[]</default>
      <summary>List of breakpoints</summary>
    </key>
  </schema>
</schemalist>
